{
  "expo": {
    "name": "survey",
    "slug": "survey",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "updates": {
      "enabled": false
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
            "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to record where this survey response was collected.",
        "NSMicrophoeDescription": "We use your microphone to record your response in our survey",
        "NSCameraUsageDescription": "We use your camera to take picture of your and your house during the surver"

      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store", "expo-image-picker"
    ]
  }
}
